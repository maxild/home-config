  <!--
  <action id="Compile">
    <keyboard-shortcut first-keystroke="control shift F9"/>
  </action>
  -->

  <!-- Build+Run+Debug commands -->
  <action id="BuildSolutionAction">
    <keyboard-shortcut first-keystroke="control B"/>
  </action>
  <action id="RebuildSolutionAction">
    <keyboard-shortcut first-keystroke="shift control B"/>
  </action>

  <action id="Run">
    <keyboard-shortcut first-keystroke="control F5"/>
  </action>

  <!-- This dialog will show the below important actions!!!  -->
  <action id="DebuggingActionsGroup">
    <keyboard-shortcut first-keystroke="control D" second-keystroke="control A"/>
    <keyboard-shortcut first-keystroke="control D" second-keystroke="A"/>
  </action>

  <!-- 'Run...' in UI -->
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="control alt R"/>
  </action>
  <!-- 'Debug...' in UI-->
  <action id="ChooseDebugConfiguration">
    <keyboard-shortcut first-keystroke="control alt D"/>
  </action>

  <!--
    From here we can run/debug all known run/debug configurations
    (for example provided by launchSettings.json)
  -->
  <action id="RunMenu">
    <keyboard-shortcut first-keystroke="control D" second-keystroke="control D"/>
    <keyboard-shortcut first-keystroke="control D" second-keystroke="D"/>
  </action>

  <!--
    Man starter ikke debugger med F5. MEN

    I stedet benytter man

    F10: Start Debugging and Step Over (StepOver)
    F11: Start Debugging and Step Into (StepInto)
    Ctrl+F10: Start Debugging and Run to Cursor (RunToCursor)
    Ctrl+Alt+F10: Start Debugging and Run to Cursor Non-Stop (ForceRunToCursor)

    You start debugging and then suspend the program in one go using the above actions
  -->
  <action id="Debug">
    <keyboard-shortcut first-keystroke="alt F5"/>
  </action>

  <action id="RunClass">
    <keyboard-shortcut first-keystroke="control F5"/>
  </action>
  <action id="Rerun">
    <keyboard-shortcut first-keystroke="control shift F5"/>
  </action>

  <!-- debugging actions -->
  <action id="Stop">
    <keyboard-shortcut first-keystroke="shift F5"/>
  </action>
  <action id="Resume">
    <keyboard-shortcut first-keystroke="F5"/>
  </action>
  <action id="StepOut">
    <keyboard-shortcut first-keystroke="shift F11"/>
  </action>
  <action id="StepInto">
    <keyboard-shortcut first-keystroke="F11"/>
  </action>
  <action id="SmartStepInto">
    <!-- TODO: Hvad er det? -->
    <keyboard-shortcut first-keystroke="shift F7"/>
  </action>
  <action id="ForceStepInto">
    <keyboard-shortcut first-keystroke="alt F11"/>
  </action>
  <action id="StepOver">
    <keyboard-shortcut first-keystroke="F10"/>
  </action>
  <action id="ForceStepOver">
    <!-- TODO: Hvad er det? -->
    <keyboard-shortcut first-keystroke="alt shift F8"/>
  </action>
  <action id="RunToCursor">
    <keyboard-shortcut first-keystroke="control F10"/>
  </action>
  <action id="ForceRunToCursor">
    <keyboard-shortcut first-keystroke="control alt F10"/>
  </action>


  <action id="EvaluateExpression">
    <keyboard-shortcut first-keystroke="shift F9"/>
  </action>
  <action id="QuickEvaluateExpression">
    <keyboard-shortcut first-keystroke="control alt F8"/>
  </action>
  <action id="ShowExecutionPoint">
    <keyboard-shortcut first-keystroke="alt F10"/>
  </action>

  <!-- Breakpoints -->
  <action id="ToggleLineBreakpoint">
    <keyboard-shortcut first-keystroke="F9"/>
  </action>
  <action id="ToggleTemporaryLineBreakpoint">
    <keyboard-shortcut first-keystroke="shift control alt F8"/>
  </action>
  <action id="ToggleBreakpointEnabled">
    <keyboard-shortcut first-keystroke="control F9"/>
  </action>
  <action id="ViewBreakpoints">
    <keyboard-shortcut first-keystroke="control shift F8"/>
  </action>
  <action id="EditBreakpoint">
    <keyboard-shortcut first-keystroke="alt F9"/>
  </action>
